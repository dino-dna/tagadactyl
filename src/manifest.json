{
  "manifest_version": 2,
  "name": "tagadactyl",
  "description": "Tag collaborators instantly in GitHub via @-mentions!",
  "version": "1.0.0",
  "options_page": "options.html",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "https://github.com/",
    "storage"
  ]
}
